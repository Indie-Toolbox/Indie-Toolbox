[
  {
    "name": "p5pj",
    "version": "0b1.0.1",
    "title": "P5.js Project Manager",
    "description": "A simple and easy to use boilerplate code generator for p5.js",
    "git": "https://github.com/pricter/p5pj",
    "supportedPlatforms": [
      "Windows",
      "Linux"
    ],
    "run": {
      "Windows": "python C:/toolbox/p5pj/p5pj-windows.py",
      "Linux": "python3 opt/toolbox/p5pj/p5pj-linux.py"
    },
    "file": {
      "Windows": "p5pj-windows.py",
      "Linux": "p5pj-linux.py"
    }
  }
]