[
  {
    "name": "p5pj",
    "version": "0b1.0.2",
    "title": "P5.js Project Manager",
    "description": "A simple and easy to use boilerplate code generator for p5.js",
    "git": "https://github.com/pricter/p5pj",
    "supportedPlatforms": [
      "Windows",
      "Linux",
      "Mac"
    ],
    "install": {
      "Windows": [],
      "Linux": [],
      "Mac": []
    },
    "run": {
      "Windows": "python C:/toolbox/p5pj/p5pj-windows.py",
      "Linux": "python3 ~/.toolboxlauncher/p5pj/p5pj-linux.py",
      "Mac": "python3 ~/.toolboxlauncher/p5pj/p5pj-linux.py"
    },
    "file": {
      "Windows": "p5pj-windows.py",
      "Linux": "p5pj-linux.py",
      "Mac": "p5pj-linux.py"
    }
  },
   {
    "name": "Lime",
    "version": "v1.0.1",
    "title": "Lime",
    "description": "Lightweight python ide",
    "git": "https://github.com/DevStuffzz/Lime",
    "supportedPlatforms": [
      "Windows",
      "Linux",
      "Mac"
    ],
    "install": {
      "Windows": [],
      "Linux": [],
      "Mac": []
    },
    "run": {
      "Windows": "java -jar C:/toolbox/lime/Lime.jar",
      "Linux": "java -jar ~/.toolboxlauncher/lime/Lime.jar",
      "Mac": "java -jar ~/.toolboxlauncher/lime/Lime.jar"
    },
    "file": {
      "Windows": "Lime.jar",
      "Linux": "Lime.jar",
      "Mac": "Lime.jar"
    }
  }
]
