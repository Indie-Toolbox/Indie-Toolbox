    "title": "P5.js Project Manager",
    "description": "A simple and easy to use boilerplate code generator for p5.js",
    "git": "https://github.com/pricter/p5pj",
    "supportedPlatforms": [
      "Windows",
      "Linux"
    ],
    "run": {
      "Windows": "python C:/toolbox/p5pj/p5pj.py",
      "Linux": "py usr/toolbox/p5pj/p5pj.py"
    },
    "file": {
      "Windows": "WHATEVER.BLAH",
      "Linux": "WHATEVER.BLAH"
    }
  }
]